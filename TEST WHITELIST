   if Aimbot and game.Players.LocalPlayer.Character:FindFirstChild(SelectWeaponPvp).RemoteFunctionShoot then
      game:GetService("Players").LocalPlayer.Character[SelectWeaponPvp].RemoteFunctionShoot:InvokeServer(Vector3.new(Px, Py, Pz),game:GetService("Players"):FindFirstChild(KillPlayer).Character.HumanoidRootPart)
   end 
end)



