 function CombatPlayer()
    pcall(function()
        for i, v in pairs(game:GetService("Workspace").Characters:GetChildren()) do
            if v.Name == KillPlayer and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") then
                repeat
                wait(0.1)
                    EquipWeapon(SelectWeaponPvp)
                    autobuso()
                    game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
                    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,Distantnum, 10)
                 --   sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
                    v.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
                    game:GetService 'VirtualUser':CaptureController()
                    game:GetService 'VirtualUser':Button1Down(Vector2.new(1280, 672))
                    Px = v.HumanoidRootPart.CFrame.X
                    Py = v.HumanoidRootPart.CFrame.Y
                    Pz = v.HumanoidRootPart.CFrame.Z
                until  v.Humanoid.Health <= 0 or pk == false
                game.Players.localPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(266.227783, 1.39509034, 1857.00732)

                pk = false
                v.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
            end
        end
    end)
 end

 spawn(function()
    while wait(0.1) do
     if pk then
        CombatPlayer()
     end
    end
 end)



