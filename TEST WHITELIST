function polefarm()
    CheckQuest()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-7917.53613, 5616.61377, -2277.78564)
    if game.Workspace.Enemies:FindFirstChild("Thunder God [Lv. 575] [Boss]") then
        local mobs = game.Workspace.Enemies:GetChildren()
        for i = 1, #mobs do 
            local v = mobs[i]
            if v.Name == "Thunder God [Lv. 575] [Boss]" and v:IsA("Model") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                return v
            end
        end
      
    end
    return game.ReplicatedStorage:FindFirstChild("Thunder God [Lv. 575] [Boss]")
end
function polev1farm()
    if _G.AutoPole and not game.Players.LocalPlayer.Backpack:FindFirstChild("Pole (1st Form)") and  not game.Players.LocalPlayer.Character:FindFirstChild("Pole (1st Form)") then
        pcall(function()
            local v = polefarm()
            EquipWeapon(_G.SelectWeapon)
            autobuso()
            if hit then
                v.HumanoidRootPart.Transparency = 1
                else
                v.HumanoidRootPart.Transparency = 0.75
                 end
                v.HumanoidRootPart.CanCollide = false
                v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
                pcall(function()
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, Instantnum, 0)
                sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
                game:GetService 'VirtualUser':CaptureController()
                game:GetService 'VirtualUser':Button1Down(Vector2.new(24, 242))
             
            end)
        
        end) 
        if _G.ServerHop == true and not game.Workspace.Enemies:FindFirstChild("Thunder God [Lv. 575] [Boss]") and  not game.ReplicatedStorage:FindFirstChild("Thunder God [Lv. 575] [Boss]") then
            Hopey() 
        end
    end
end

spawn(function()
    while wait(.1) do
        if _G.AutoPole then
            polev1farm()
        end
        -- Magnet = false
    end
end)
 MainFarm:Label("╚═══════════════════════════════════════════╝")


function mobbot()
    CheckQuest()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2848.59399, 7.4272871, 5342.44043)
    if game.Workspace.Enemies:FindFirstChild("Mob Leader [Lv. 120] [Boss]") then
        local mobs = game.Workspace.Enemies:GetChildren()
        for i = 1, #mobs do 
            local v = mobs[i]
            if v.Name == "Mob Leader [Lv. 120] [Boss]" and v:IsA("Model") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                return v
            end
        end
      
    end
    return game.ReplicatedStorage:FindFirstChild("Mob Leader [Lv. 120] [Boss]")
end

function autosaber()
    chekvel = game:GetService("Players").LocalPlayer.Data.Level.Value
   if game:GetService("Workspace").Map.Jungle.Final.Invis.Transparency == 0 and chekvel >= 200 then
    _G.AutoFarm = false
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1180.89563, 21.0007095, 187.861374)
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame= CFrame.new(-1421.19995, 44.5000229, 21.6000061)
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame= CFrame.new(-1648.49451, 19.5000286, 437.794678)
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame= CFrame.new(-1324.10144, 31.4560413, -461.404114)
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame= CFrame.new(-1152.16479, -1.20998275, -700.431641)
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame= CFrame.new(-1610.00757, 11.5049858, 164.001587)
    local args = {
        [1] = "ProQuestProgress",
        [2] = "GetTorch"
    }
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))

