if placeId == 2753915549 then
    OldWorld = true
elseif placeId == 4442272183 then
    newworld = true
end
function autobuso()
    if not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
    end
    if OldWorld and _G.AutoNewworld and game.Players.localPlayer.Data.Level.Value >= 700 and
        not game.Players.LocalPlayer.Character:FindFirstChild("HasBuso") then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
    end
end
function factoryfarm()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(448.46756, 199.356781, -441.389252)
     if game.Workspace.Enemies:FindFirstChild("Core") then
         local mobs = game.Workspace.Enemies:GetChildren()
         for i = 1, #mobs do 
             local v = mobs[i]
             if v.Name == "Core" and v:IsA("Model") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                
                 return v
             end
         end
       
        end 
     return game.ReplicatedStorage:FindFirstChild("Core")
 end

spawn(function()
    while wait(.1) do
       if _G.AutoFactory then
          if game.Workspace.Enemies:FindFirstChild("Core") or game.ReplicatedStorage:FindFirstChild("Core") then
             pcall(function() 
             _G.AutoFarm = false
             local v = factoryfarm()
             EquipWeapon(_G.SelectWeapon)
             autobuso()
             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(448.46756, 199.356781, -441.389252)
             --setsimulationradius(1e+1598, 1e+1599)
             sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
             game:GetService 'VirtualUser':CaptureController()
             game:GetService 'VirtualUser':Button1Down(Vector2.new(24, 242))
             end)
          elseif  game.ReplicatedStorage:FindFirstChild("Core") then
             _G.AutoFarm = false
             game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(448.46756, 199.356781, -441.389252)
          elseif openfac then
             _G.AutoFarm = true
          end
       end 
    end
 end)

loadstring(game:HttpGet "https://raw.githubusercontent.com/Root-Hub-Dev/BloxFruit_Script/main/Quest.txt")()
CheckQuest()

spawn(function()
    while wait(.1) do
        if _G.AutoNewworld and OldWorld then
            local MyLevel = game.Players.localPlayer.Data.Level.Value
            if MyLevel >= 700 and OldWorld then
                _G.AutoFarm = false
                _G.SelectWeapon = "Key"
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(4849.29883, 5.65138149, 719.611877)
                wait(0.5)
                local args = {
                    [1] = "DressrosaQuestProgress",
                    [2] = "Detective"
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                wait(0.5)
                if game.Players.LocalPlayer.Backpack:FindFirstChild("Key") then
                    local tool = game.Players.LocalPlayer.Backpack:FindFirstChild("Key")
                    wait(.4)
                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
                end
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1347.7124, 37.3751602, -1325.6488)
                wait(0.5)
                function click()
                    game:GetService("VirtualUser"):CaptureController()
                    game:GetService("VirtualUser"):Button1Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
                end
                if _G.AutoNewworld and game.Workspace.Enemies:FindFirstChild("Ice Admiral [Lv. 700] [Boss]") and
                    game.Workspace.Map.Ice.Door.CanCollide == false and game.Workspace.Map.Ice.Door.Transparency == 1 then
                    CheckBoss = true
                    _G.SelectWeapon = SelectWeaponOld
                    for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if CheckBoss and v:IsA("Model") and v:FindFirstChild("Humanoid") and
                            v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 and v.Name == "Ice Admiral [Lv. 700] [Boss]" then
                            repeat
                                wait(.1)
                                pcall(function()
                                    autobuso()
                                    EquipWeapon(_G.SelectWeapon)
                                    game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
                                    v.HumanoidRootPart.Size = Vector3.new(50, 50, 50)
                                    if hit then
                                        v.HumanoidRootPart.Transparency = 1
                                        else
                                        v.HumanoidRootPart.Transparency = 0.75
                                         end
                                    v.HumanoidRootPart.CanCollide = false
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,Instantnum, 0)
                                    click()
                                end)
                            until not CheckBoss or not v.Parent or v.Humanoid.Health <= 0 or _G.AutoNewworld == false
                        end
                    end
                    CheckBoss = false
                    wait(0.5)
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1166.23743, 7.65220165, 1728.36487)
                    wait(0.5)
                    -- OLD WORLD to NEW WORLD
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelDressrosa")
                else
                    if game.Players.LocalPlayer.Backpack:FindFirstChild("Key") then
                        local tool = game.Players.LocalPlayer.Backpack:FindFirstChild("Key")
                        wait(.4)
                        game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
                    end
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =  CFrame.new(1347.7124, 37.3751602, -1325.6488)
                end
            end
        end
    end
end)
local LocalPlayer = game:GetService("Players").LocalPlayer
function enemy()
    CheckQuest()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameMon
    if game.Workspace.Enemies:FindFirstChild(mob) then
        local mobs = game.Workspace.Enemies:GetChildren()
        for i = 1, #mobs do 
            local v = mobs[i]
            if v.Name == mob and v:IsA("Model") and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                return v
            end
            Magnet = false
        end
    end
    return game.ReplicatedStorage:FindFirstChild(mob)
end
function invi()
    CheckQuest()
    if game.Workspace.Enemies:FindFirstChild(mob) then
        local mobs = game.Workspace.Enemies:GetDescendants()
        for i = 1, #mobs do 
            local v = mobs[i]
            if v.Name == "HumanoidRootPart" then
                v.Transparency = 1
                elseif v:IsA("BasePart") or v:IsA("MeshPart") then
                v.Transparency = 1
                v.BrickColor = BrickColor.new("White")
                elseif v.ClassName == "Decal" and v.Name == "face" then
                v.Texture = ""
                elseif v.ClassName == "Mesh" then
                v.MeshId = ""
                end
        end
    end
end 

function autofarm()
    if LocalPlayer.PlayerGui.Main.Quest.Visible == false then
        CheckQuest()
        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameQuest
        Magnet = false
        CheckQuest()
        wait(1)
        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameQuest
        wait(1.1)
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NaemQuest, LevelQuest)
    elseif game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
        CheckQuest()
        if string.find(LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, NameMon) then
        pcall(function()
            local v = enemy()
            EquipWeapon(_G.SelectWeapon)
            autobuso()
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            pcall(function()
                --setsimulationradius(10000000000, 1000000000000)
              --  setsimulationradius(1e+1598, 1e+1599)
            --  game:GetService("Players").LocalPlayer.SimulationRadius = math.huge
             --   sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, Instantnum, 0) 
               
            
              v.Humanoid.PlatformStand = true
              game:GetService 'VirtualUser':CaptureController()
              game:GetService 'VirtualUser':Button1Down(Vector2.new(24, 242))
              game:GetService'VirtualUser':CaptureController()
              game:GetService'VirtualUser':Button1Down(Vector2.new(31, 123))
               --game:GetService 'VirtualUser':CaptureController()
               --game:GetService 'VirtualUser':Button1Down(Vector2.new(24, 242))
                Magnet = true 
                PosMon = v.HumanoidRootPart.CFrame
                
             
            end)
            
        end) 
    else
    Magnet = false
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
    CheckQuest()
    LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameQuest
    wait(1.5)
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NaemQuest,LevelQuest)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameMon
    end
    
end
end

game:GetService("RunService").RenderStepped:connect(
   function()
  settings().Physics.AllowSleep = false
 --settings().Physics.ThrottleAdjustTime = math.huge - math.huge
  --sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
  --setsimulationradius(math.huge*math.huge*math.huge)
  --sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius",math.huge)
   sethiddenproperty(game.Players.LocalPlayer,"SimulationRadius", math.huge)

end)
function bringallmob(kai)
    pcall(function()
    
    if game.Workspace.Enemies:FindFirstChild(kai) then
        local mobs = game.Workspace.Enemies:GetChildren()
        for i = 1, #mobs do 
            local v = mobs[i]
            if v.Name == kai and v:IsA("Model") and v:FindFirstChild("Humanoid")  then
               -- v.Busy.Archivable = false 
               -- v.Humanoid.Archivable = false 
               -- v.HumanoidRootPart.Archivable = false
                if bring then
                v.HumanoidRootPart.CFrame = PosMon * CFrame.new(0, 0, 2) 
                   
                  --  v.Humanoid.PlatformStand = true
                end
               if hit then
                v.HumanoidRootPart.Transparency = 1
                else
                v.HumanoidRootPart.Transparency = 0.75
                 end

                 v.HumanoidRootPart.CanCollide = false
                
                 v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)     
                end
            end
        end
      
end)
end
