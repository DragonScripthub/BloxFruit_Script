local Playerhi = MainFrame2:Dropdown("Select Player For Copy Clothes",PlayerName,function(t) 
    KillPlayer = t
 end)

 MainFrame2:Button("Refresh Player",function()
    PlayerName = {}
    Playerhi:Clear()
    for i,v in pairs(game.Players:GetChildren()) do  
        Playerhi:Add(v.Name)
    end
 end)
 MainFrame2:Button("Copy",function()
    local pname = KillPlayer
    local target = game.Players[pname]
    local targetCharacter = target.Character
    local myCharacter = game.Players.LocalPlayer.Character
    
    local targetShirt = targetCharacter:FindFirstChild("Shirt")
    local targetPants = targetCharacter:FindFirstChild("Pants")
    local targetBodyColor = targetCharacter:FindFirstChild("Body Colors")
    local targetT_Shirt = targetCharacter:FindFirstChild("Shirt Graphic")
    
    local myShirt = myCharacter:FindFirstChild("Shirt")
    local myPants = myCharacter:FindFirstChild("Pants")
    local myBodyColor = myCharacter:FindFirstChild("Body Colors")
    local myT_Shirt = myCharacter:FindFirstChild("Shirt Graphic")
    
    
    function stripmenakeddaddy()
    myShirt:Remove()
    myPants:Remove()
    myBodyColor:Remove()
    if myT_Shirt then
    myT_Shirt:Remove()
    end
    --[[ for ind,val in pairs(myCharacter:GetChildren()) do
    if val:IsA("Accessory") then
    val:Destroy()
    end
    end
    --]]
    end
    
    function IAmASpyFromTF2()
    if targetShirt then
    local boe = targetShirt:Clone()
    boe.Parent = myCharacter
    end
    if targetPants then
    local boee = targetPants:Clone()
    boee.Parent = myCharacter
    end
    if targetBodyColor then
    local tbc = targetBodyColor:clone()
    tbc.Parent = myCharacter
    end
    for i,v in pairs(targetCharacter:GetChildren()) do
    if v:IsA("Accessory") then
    v:Clone()
    v.Parent = myCharacter
    end
    end
    end
    
    stripmenakeddaddy()
    wait(.05)
    IAmASpyFromTF2()

 end)

