function TP(Object) -- Object = part teleporting to.
    local tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(0,Enum.EasingStyle.Linear) -- change the number to a higher number if you get kicked for TP.
    local tween = tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(Object.Position)})
    tween:Play()  
end
spawn(function()
    while wait(.1) do
       if tpchest then
        for i,v in pairs(game.Workspace:GetChildren()) do
            if v.Name == "Chest1" or v.Name == "Chest2" or v.Name == "Chest3" and v:IsA "Part" then
            TP(v)
            end 
        end
       end 
    end
end)]]
