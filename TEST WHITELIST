end)




function autofarmgun()
    if AutoFarmgun then
        CheckMob()
        pcall(function()
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Telemon
            for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                if v.Name == SelectMob and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") then
                    HP = v.Humanoid.Health
                    repeat
                        wait()
                                x = v.HumanoidRootPart.CFrame.X
                                y = v.HumanoidRootPart.CFrame.Y
                                z = v.HumanoidRootPart.CFrame.Z
                        autobuso()
                        if v.Humanoid.Health > HP * Healthde / 100 then
                            EquipWeapon(SelectWeaponmon)
                            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame =
                                v.HumanoidRootPart.CFrame * CFrame.new(0, Instantnum, 0)
                            Magnet = true
                            PosMon = v.HumanoidRootPart.CFrame
                         --  setsimulationradius(1e+1598, 1e+1599)
                            sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
                            game:GetService 'VirtualUser':CaptureController()
                            game:GetService 'VirtualUser':Button1Down(Vector2.new(1280, 672))
                            -- v.HumanoidRootPart.CanCollide = false
                        elseif v.Humanoid.Health <=  HP * Healthde / 100 then
                            repeat
                                wait()
                                game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
                                EquipWeapon(SelectWeagun)
                                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, Instantnum, 0)
                                game:GetService 'VirtualUser':CaptureController()
                                game:GetService 'VirtualUser':Button1Down(Vector2.new(1280, 672))
                                Magnet = true
                                PosMon = v.HumanoidRootPart.CFrame
                                sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
                                if SkillZ and game:GetService("Players").LocalPlayer.PlayerGui.Main.Skills[SelectWeagun].Z.Cooldown.AbsoluteSize.X == 0 then
                                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeagun).RemoteEvent:FireServer(Vector3.new(x, y, z))
                                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeagun).RemoteFunction:InvokeServer("Z", Vector3.new(x, y, z))
                                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeagun).RemoteEvent:FireServer(Vector3.new(x, y, z))
                                elseif SkillX and game:GetService("Players").LocalPlayer.PlayerGui.Main.Skills[SelectWeagun].X.Cooldown.AbsoluteSize.X == 0 then
                                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeagun).RemoteEvent:FireServer(Vector3.new(x, y, z))
                                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeagun).RemoteFunction:InvokeServer("X", Vector3.new(x, y, z))
                                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeagun).RemoteEvent:FireServer(Vector3.new(x, y, z))
                                end
                            until v.Humanoid.Health <= 0 or AutoFarmgun == false or v.Humanoid.Health > HP * Healthde / 100

                        end
                    until v.Humanoid.Health <= 0 or AutoFarmgun == false
                end
                Magnet = false
               -- game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Telemon
                -- game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Telemon
            end
        end)
    end
end
spawn(function()
    local LocalPlayer = game:GetService("Players").LocalPlayer
    while wait(.1) do
        if AutoFarmgun and Magnet then
            pcall(function()
                repeat
                    wait(.1)
                    for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if v.Name == SelectMob then
                            if hit then
                                v.HumanoidRootPart.Transparency = 1
                                else
                                v.HumanoidRootPart.Transparency = 0.75
                                 end
                            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
                            v.HumanoidRootPart.CanCollide = false
                            -- game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
                            v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                            if bring then
                                v.HumanoidRootPart.CFrame = PosMon
                            end
                        end
                    end

                until AutoFarmgun == false or Magnet == false
            end)
        end
    end
end)
spawn(function()
    while wait(.1) do
        if AutoFarmgun then  
            autofarmgun()
        end
        -- Magnet = false
    end
end)

local lp1 = game:GetService('Players').LocalPlayer
local mouse1 = lp1:GetMouse()
mouse1.Button1Down:Connect(function()
   if AutoFarmgun and game.Players.LocalPlayer.Character:FindFirstChild(SelectWeagun).RemoteFunctionShoot then
       sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
      game:GetService("Players").LocalPlayer.Character[SelectWeagun].RemoteFunctionShoot:InvokeServer(Vector3.new(x, y, z),game.Workspace.Enemies:FindFirstChild(SelectMob).HumanoidRootPart)
   end 
end)


function FarmmasAuto()
  
   CheckMob()
   game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Telemon
    if game.Workspace.Enemies:FindFirstChild(SelectMob) then
        local mobs = game.Workspace.Enemies:GetChildren()
        for i = 1, #mobs do 
            local v = mobs[i]
            if v.Name == SelectMob and v:IsA("Model") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
             
                return v
            end
        end
      
    end
    return game.ReplicatedStorage:FindFirstChild(SelectMob)

end
function fruitfarmmas()
    local v = FarmmasAuto()
    pcall(function()
   --     CheckMob()
  --  game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Telemon
   
    game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
     if v.Humanoid.Health >=  v.Humanoid.MaxHealth * Healthde / 100 then
    autobuso()
    EquipWeapon(SelectWeaponmon)
    sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, Instantnum, 0)
    game:GetService 'VirtualUser':CaptureController()
    game:GetService 'VirtualUser':Button1Down(Vector2.new(1280, 672))
    Magnet = true
    PosMon = v.HumanoidRootPart.CFrame
    
     elseif v.Humanoid.Health <=  v.Humanoid.MaxHealth * Healthde / 100 then
        EquipWeapon(SelectWeaFruit)
            sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, Instantnum, 0)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            Magnet = true
            PosMon = v.HumanoidRootPart.CFrame
         --  setsimulationradius(1e+1598, 1e+1599)
           --[[  xx = v.HumanoidRootPart.CFrame.X
             yy = v.HumanoidRootPart.CFrame.Y
             zz = v.HumanoidRootPart.CFrame.Z]]

            if SkillZ and game:GetService("Players").LocalPlayer.PlayerGui.Main.Skills[SelectWeaFruit].Z.Cooldown.AbsoluteSize.X == 0 and game:GetService("Players").LocalPlayer.PlayerGui.Main.Skills[SelectWeaFruit].Z.Title.TextColor3 == Color3.fromRGB(255, 255, 255)  then
                    pcall(function()
                        
                        keypress(0x5A)
                     --  game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeaFruit).RemoteEvent:FireServer(Vector3.new(xx, yy, zz))
                       wait(.5)
                       game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeaFruit).RemoteEvent:FireServer(Vector3.new(v.HumanoidRootPart.CFrame.X, v.HumanoidRootPart.CFrame.Y, v.HumanoidRootPart.CFrame.Z))
                        keyrelease(0x5A)
                    end)
        end
            if SkillX and game:GetService("Players").LocalPlayer.PlayerGui.Main.Skills[SelectWeaFruit].X.Cooldown.AbsoluteSize.X == 0 and game:GetService("Players").LocalPlayer.PlayerGui.Main.Skills[SelectWeaFruit].X.Title.TextColor3 == Color3.fromRGB(255, 255, 255)  then
                pcall(function()
                    keypress(0x58)
              --      game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeaFruit).RemoteEvent:FireServer(Vector3.new(xx, yy, zz))
                   wait(.5)
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeaFruit).RemoteEvent:FireServer(Vector3.new(v.HumanoidRootPart.CFrame.X, v.HumanoidRootPart.CFrame.Y, v.HumanoidRootPart.CFrame.Z))
                    keyrelease(0x58)
                end)
        end
            if SkillC and game:GetService("Players").LocalPlayer.PlayerGui.Main.Skills[SelectWeaFruit].C.Cooldown.AbsoluteSize.X == 0 and game:GetService("Players").LocalPlayer.PlayerGui.Main.Skills[SelectWeaFruit].C.Title.TextColor3 == Color3.fromRGB(255, 255, 255) then
                pcall(function()
                    keypress(0x43)
                 --   game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeaFruit).RemoteEvent:FireServer(Vector3.new(xx, yy, zz))
                  wait(.5)
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeaFruit).RemoteEvent:FireServer(Vector3.new(v.HumanoidRootPart.CFrame.X, v.HumanoidRootPart.CFrame.Y, v.HumanoidRootPart.CFrame.Z))
                    keyrelease(0x43)
                end)
        end
            if SkillV and game:GetService("Players").LocalPlayer.PlayerGui.Main.Skills[SelectWeaFruit].V.Cooldown.AbsoluteSize.X == 0 and game:GetService("Players").LocalPlayer.PlayerGui.Main.Skills[SelectWeaFruit].V.Title.TextColor3 == Color3.fromRGB(255, 255, 255) then
                pcall(function()
                    keypress(0x56)
                  --  game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeaFruit).RemoteEvent:FireServer(Vector3.new(xx, yy, zz))
                  wait(.5)
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(SelectWeaFruit).RemoteEvent:FireServer(Vector3.new(v.HumanoidRootPart.CFrame.X, v.HumanoidRootPart.CFrame.Y, v.HumanoidRootPart.CFrame.Z))
                    keyrelease(0x56)
                end)

            end
    end
        
end)

end
