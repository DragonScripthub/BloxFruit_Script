function bartofarm()
    CheckQuest()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(970.369446, 142.653198, 1217.3667, 0.162079468, -4.85452638e-08, -0.986777723, 1.03357589e-08, 1, -4.74980872e-08, 0.986777723, -2.50063148e-09, 0.162079468)
    if game.Workspace.Enemies:FindFirstChild("Swan Pirate [Lv. 775]") then
        local mobs = game.Workspace.Enemies:GetChildren()
        for i = 1, #mobs do 
            local v = mobs[i]
            if v.Name == "Swan Pirate [Lv. 775]" and v:IsA("Model") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                return v
            end
        end
      
    end
    return game.ReplicatedStorage:FindFirstChild("Swan Pirate [Lv. 775]")
end
function bartojera()
    CheckQuest()
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2203.76953, 448.966034, 752.731079, -0.0217453763, 0, -0.999763548, 0, 1, 0, 0.999763548, 0, -0.0217453763)
    if game.Workspace.Enemies:FindFirstChild("Jeremy [Lv. 850] [Boss]") then
        local mobs = game.Workspace.Enemies:GetChildren()
        for i = 1, #mobs do 
            local v = mobs[i]
            if v.Name == "Jeremy [Lv. 850] [Boss]" and v:IsA("Model") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                return v
            end
        end
      
    end
    return game.ReplicatedStorage:FindFirstChild("Jeremy [Lv. 850] [Boss]")
end
function Barto()
    if barto == "true" and game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-460.794342, 73.0200958, 298.641266, 0.906751215, -0.000221123279, 0.421665549, 9.19660451e-05, 0.99999994, 0.000326640118, -0.421665609, -0.000257402455, 0.906751156)
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo")
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest","BartiloQuest",1)
    wait(2)
    end
    barto = "false"
    if Bartiloquest and game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
    pcall(function()
        local v = bartofarm()
        EquipWeapon(SelectWeabarlo)
        autobuso()
        game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
        pcall(function()
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, Instantnum, 0)
            sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
            game:GetService 'VirtualUser':CaptureController()
            game:GetService 'VirtualUser':Button1Down(Vector2.new(24, 242))
            Magnet = true
            PosMon = v.HumanoidRootPart.CFrame
        end)
    end) 
elseif game.ReplicatedStorage:FindFirstChild("Jeremy [Lv. 850] [Boss]") or game.workspace.Enemies:FindFirstChild("Jeremy [Lv. 850] [Boss]") then
    Magnet = false
    pcall(function()
        v = bartojera()
        popop = "true"
        EquipWeapon(SelectWeabarlo)
        autobuso()
        v.HumanoidRootPart.CanCollide = false
        v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
        game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
        pcall(function()
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, Instantnum, 0)
            sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
            game:GetService 'VirtualUser':CaptureController()
            game:GetService 'VirtualUser':Button1Down(Vector2.new(24, 242))
        end)
    end) 
elseif popop == "true" and  not game.workspace.Enemies:FindFirstChild("Jeremy [Lv. 850] [Boss]") then
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.Dressrosa.BartiloPlates.Plate1.CFrame
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.Dressrosa.BartiloPlates.Plate2.CFrame
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.Dressrosa.BartiloPlates.Plate3.CFrame
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.Dressrosa.BartiloPlates.Plate4.CFrame
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.Dressrosa.BartiloPlates.Plate5.CFrame
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.Dressrosa.BartiloPlates.Plate6.CFrame
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.Dressrosa.BartiloPlates.Plate7.CFrame
    wait(2)
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.Dressrosa.BartiloPlates.Plate8.CFrame
    popop = "false"
end

end

spawn(function()
    local LocalPlayer = game:GetService("Players").LocalPlayer
    while wait(.1) do
        if Bartiloquest and Magnet then
            CheckQuest()
            pcall(function()
                repeat
                    wait(.1)
                    for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if v.Name == "Swan Pirate [Lv. 775]" then
                            wait(0.1)
                            if hit then
                                v.HumanoidRootPart.Transparency = 1
                                else
                                v.HumanoidRootPart.Transparency = 0.75
                                 end
                        --   game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
                            v.HumanoidRootPart.CanCollide = false   
                            v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                            if bring then
                                v.HumanoidRootPart.CFrame = PosMon
                            end
                       
                        end
                    end

                until LocalPlayer.PlayerGui.Main.Quest.Visible == false or Bartiloquest == false or Magnet == false
            end)
        end
    end
end)
spawn(function()
    while wait(.1) do
        if Bartiloquest then
            Barto()
        end
    end
end)

