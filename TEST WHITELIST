    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame= CFrame.new(1111.37097, 4.9214654, 4352.79688, 0.653583765, 1.48724888e-09, -0.756854177, -1.05342446e-09, 1, 1.05535225e-09, 0.756854177, 1.07527612e-10, 0.653583765)
    wait(5)
    local args = {
