function access()
    if itemas == "Ghoul Mask" then
        accessory = "GhoulMask"
        
    elseif itemas == "Black Cape" then
        accessory = "BlackCape"
        
     elseif itemas == "Black Spikey Coat" then
        accessory = "BlackSpikeyCoat"
        
     elseif itemas == "Blue Spikey Coat" then
        accessory = "BlueSpikeyCoat"
        
     elseif itemas == "Choppa" then
        accessory = "Chopper"
        
     elseif itemas == "Coat" then
        accessory = "PirateCape"
        
     elseif itemas == "Cool Shades" then
        accessory = "CoolShades"
        
     elseif itemas == "Dark Coat" then
        accessory = "DarkCoat"
    
    elseif itemas == "Marine Cap" then
        accessory = "MarineCap"
    
    elseif itemas == "Pink Coat" then
        accessory = "PinkCape"
    
    elseif itemas == "Red Spikey Coat" then
        accessory = "RedSpikeyCape"
        
    elseif itemas == "Swan Glasses" then
        accessory = "SwanGlasses"
   
    elseif itemas == "Swan Glasses" then
        accessory = "SwanGlasses"
    
    elseif itemas == "Top Hat" then
        accessory = "SaboTopHat"
        
    elseif itemas == "Usoap's Hat" then
        accessory = "UsoapHat"
        
    elseif itemas == "Warrior Helmet" then
        accessory = "Corrida"
        
    elseif itemas == "Zebra Cap" then
        accessory = "ZebraCap"
    
    elseif itemas == "Tomoe Ring" then
        accessory = "TomoeRing"
    
    elseif itemas == "Swordsman Hat" then
        accessory = "SwordsmanHat"
    
    elseif itemas == "Zebra Cap" then
        accessory = "ZebraCap"

    elseif itemas == "Zebra Cap" then
        accessory = "ZebraCap"
        
    end
    
    end
    function EquipAccess(ToolSe)
        if game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe) then
            local tool = game.Players.LocalPlayer.Backpack:FindFirstChild(ToolSe)
            wait(0.4)
            game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
           
        end
    end
    spawn(function()
    while wait(0.2) do
        if _G.AutoAccessory then
            pcall(function()
            for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if v:IsA("Tool") and v.ToolTip == "Wear" then
                 itemas = v.Name
                 access()
                     if not game.Players.LocalPlayer.Character:FindFirstChild(accessory) then
                        wait(2)
                        EquipAccess(itemas)
                        game:GetService("Players").LocalPlayer.Character:FindFirstChild(itemas).RemoteFunction:InvokeServer()
                      
                     end
                 end
                 end
                end)
        end
    end
end)
