        pcall(function()
            local v = Bosswafarm()
            EquipWeapon(SelectWeali)
            autobuso()
            if hit then
                v.HumanoidRootPart.Transparency = 1
                else
                v.HumanoidRootPart.Transparency = 0.75
                 end
            v.HumanoidRootPart.CanCollide = false
            v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            pcall(function()
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, Instantnum, 0)
                sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge)
                game:GetService 'VirtualUser':CaptureController()
                game:GetService 'VirtualUser':Button1Down(Vector2.new(24, 242))
                
            end)
        end) 
        if  game.Players.LocalPlayer.Backpack:FindFirstChild("Water Key") or game.Players.LocalPlayer.Character:FindFirstChild("Water Key") then
            EquipWeapon("Water Key")
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame= CFrame.new(-2602.64355, 239.294647, -10316.5586, 0.0310906507, 6.8576762e-09, -0.999516547, -8.93668584e-10, 1, 6.8331949e-09, 0.999516547, 6.80788093e-10, 0.0310906507)
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate",true)

        end
    end
end

spawn(function()
    while wait(.1) do
        if Autowater then
            waterKey()
        end
    end
end)


function seabe()
    if  Autosea then
        for i, v in pairs(game.Workspace.SeaBeasts:GetChildren()) do
        if string.find(v.Name, "SeaBeast")  then
        ggt = v.Name
        pcall(function()
            repeat
                wait(0.2)
            EquipWeapon(_G.SelectWeapon)
            autobuso()
            v.HumanoidRootPart.CanCollide = false
            v.HumanoidRootPart.Size = Vector3.new(60, 60, 60)    
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 100, 0)
                sethiddenproperty(game:GetService("Players").LocalPlayer,"SimulationRadius", math.huge) 
                xxx = v.HumanoidRootPart.CFrame.X
                yyy = v.HumanoidRootPart.CFrame.Y
                zzz = v.HumanoidRootPart.CFrame.Z 
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteEvent:FireServer(Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteFunction:InvokeServer("Z", Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteEvent:FireServer(Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteEvent:FireServer(Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteFunction:InvokeServer("X", Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteEvent:FireServer(Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteEvent:FireServer(Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteFunction:InvokeServer("C", Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteEvent:FireServer(Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteEvent:FireServer(Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteFunction:InvokeServer("V", Vector3.new(xxx, yyy, zzz))
                    game:GetService("Players").LocalPlayer.Character:FindFirstChild(_G.SelectWeapon).RemoteEvent:FireServer(Vector3.new(xxx, yyy, zzz))
        until v.Health.Value <= 0 or Autosea == false
        end) 
    end
    end
    end
end
spawn(function()
    while wait(.1) do
        if Autosea then
            seabe()
        end
    end
end)
local RunService = game:GetService("RunService")
local connection5    
function beat(step)
    if Autosea then
        game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
    else
        connection5:Disconnect()
    end
    
end
spawn(function()
    while wait(.1) do
       if Autosea and string.find(ggt, "SeaBeast") then
        connection5 = RunService.Heartbeat:Connect(beat)
       end
    end
 end)
